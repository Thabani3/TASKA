@model IEnumerable<TaskA.Models.QRCode>

@{
    ViewBag.Title = "Index";

}
<br />
<br />
<br />
<br />
<br />
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Order.Tasker_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QRCodeText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QRCodeImagePath)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Order.Tasker_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QRCodeText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QRCodeImagePath)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.QRId }) |
                @Html.ActionLink("Details", "Details", new { id = item.QRId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.QRId })
            </td>
        </tr>
    }

</table>
